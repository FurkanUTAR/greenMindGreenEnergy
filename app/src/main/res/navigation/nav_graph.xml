<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.greenmindgreenenergy.splashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_girisSayfaFragment"
            app:destination="@id/girisSayfaFragment" />
    </fragment>
    <fragment
        android:id="@+id/girisSayfaFragment"
        android:name="com.example.greenmindgreenenergy.girisSayfaFragment"
        android:label="fragment_giris_sayfa"
        tools:layout="@layout/fragment_giris_sayfa" >
        <action
            android:id="@+id/action_girisSayfaFragment_to_anaSayfaFragment"
            app:destination="@id/anaSayfaFragment" />
        <action
            android:id="@+id/action_girisSayfaFragment_to_hakkimizdaSayfaFragment"
            app:destination="@id/hakkimizdaSayfaFragment" />
        <action
            android:id="@+id/action_girisSayfaFragment_to_surdurulebilirEnerjiFragment"
            app:destination="@id/surdurulebilirEnerjiFragment" />
        <action
            android:id="@+id/action_girisSayfaFragment_to_robotikVeKodlamaFragment"
            app:destination="@id/robotikVeKodlamaFragment" />
    </fragment>
    <fragment
        android:id="@+id/anaSayfaFragment"
        android:name="com.example.greenmindgreenenergy.anaSayfaFragment"
        android:label="fragment_ana_sayfa"
        tools:layout="@layout/fragment_ana_sayfa" />
    <fragment
        android:id="@+id/hakkimizdaSayfaFragment"
        android:name="com.example.greenmindgreenenergy.hakkimizdaSayfaFragment"
        android:label="fragment_hakkimizda_sayfa"
        tools:layout="@layout/fragment_hakkimizda_sayfa" />
    <fragment
        android:id="@+id/surdurulebilirEnerjiFragment"
        android:name="com.example.greenmindgreenenergy.surdurulebilirEnerjiFragment"
        android:label="surdurulebilirEnerjiFragment" >
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_surdurulebilirEnerjiGirisFragment"
            app:destination="@id/surdurulebilirEnerjiGirisFragment" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_yesilEnerjiKaynaklariFragment"
            app:destination="@id/yesilEnerjiKaynaklariFragment" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_fosilYakitlarinEtkileriFragment"
            app:destination="@id/fosilYakitlarinEtkileriFragment" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_enerjiVerimliligiTeknikleriFragment"
            app:destination="@id/enerjiVerimliligiTeknikleriFragment" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_gunesPanelleriTeknolojisi"
            app:destination="@id/gunesPanelleriTeknolojisi" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_ruzgarTurbinleriVeCalismaPrensipleriFragment"
            app:destination="@id/ruzgarTurbinleriVeCalismaPrensipleriFragment" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_hidroelektrikEnerjiVeBarajlarFragment"
            app:destination="@id/hidroelektrikEnerjiVeBarajlarFragment" />
        <action
            android:id="@+id/action_surdurulebilirEnerjiFragment_to_yesilBinalarVeEnerjiYonetimiFragment"
            app:destination="@id/yesilBinalarVeEnerjiYonetimiFragment" />
    </fragment>
    <fragment
        android:id="@+id/robotikVeKodlamaFragment"
        android:name="com.example.greenmindgreenenergy.robotikVeKodlamaFragment"
        android:label="robotikVeKodlamaFragment" />
    <fragment
        android:id="@+id/surdurulebilirEnerjiGirisFragment"
        android:name="com.example.greenmindgreenenergy.surdurulebilirEnerjiGirisFragment"
        android:label="surdurulebilirEnerjiGirisFragment" />
    <fragment
        android:id="@+id/yesilEnerjiKaynaklariFragment"
        android:name="com.example.greenmindgreenenergy.yesilEnerjiKaynaklariFragment"
        android:label="yesilEnerjiKaynaklariFragment" />
    <fragment
        android:id="@+id/fosilYakitlarinEtkileriFragment"
        android:name="com.example.greenmindgreenenergy.fosilYakitlarinEtkileriFragment"
        android:label="fosilYakitlarinEtkileriFragment" />
    <fragment
        android:id="@+id/enerjiVerimliligiTeknikleriFragment"
        android:name="com.example.greenmindgreenenergy.enerjiVerimliligiTeknikleriFragment"
        android:label="enerjiVerimliligiTeknikleriFragment" />
    <fragment
        android:id="@+id/gunesPanelleriTeknolojisi"
        android:name="com.example.greenmindgreenenergy.gunesPanelleriTeknolojisi"
        android:label="fragment_gunes_panelleri_teknolojisi"
        tools:layout="@layout/fragment_gunes_panelleri_teknolojisi" />
    <fragment
        android:id="@+id/ruzgarTurbinleriVeCalismaPrensipleriFragment"
        android:name="com.example.greenmindgreenenergy.ruzgarTurbinleriVeCalismaPrensipleriFragment"
        android:label="ruzgarTurbinleriVeCalismaPrensipleriFragment" />
    <fragment
        android:id="@+id/hidroelektrikEnerjiVeBarajlarFragment"
        android:name="com.example.greenmindgreenenergy.hidroelektrikEnerjiVeBarajlarFragment"
        android:label="fragment_hidroelektrik_enerji_ve_barajlar"
        tools:layout="@layout/fragment_hidroelektrik_enerji_ve_barajlar" />
    <fragment
        android:id="@+id/yesilBinalarVeEnerjiYonetimiFragment"
        android:name="com.example.greenmindgreenenergy.yesilBinalarVeEnerjiYonetimiFragment"
        android:label="yesilBinalarVeEnerjiYonetimiFragment" />
</navigation>